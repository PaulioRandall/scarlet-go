
// Types
a := `abc`;
b := "xyz";
c, d := TRUE, FALSE
e, f := 123, 123.789
g := [
	123, c,
	[a, "xyz"],
]
h := F(a, b -> c, d) DO
	c := a + b
	d := F(a -> b) DO
		b := 1
	END
END

// Multiple return & assignment
x, y := c, d

// Removing variables
x, y := _, _

// Function call
x, y := h(e, f)

// Arithmetic
two := 1 + 1
three := 1.5 + 1.5
twelve := 3 * 3 + 3 // Error
